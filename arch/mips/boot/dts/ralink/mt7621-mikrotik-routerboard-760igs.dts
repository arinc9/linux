/dts-v1/;

#include "mt7621.dtsi"

#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	compatible = "mikrotik,routerboard-760igs", "mediatek,mt7621-soc";
	model = "MikroTik RouterBOARD 760iGS";

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	keys: keys {
		compatible = "gpio-keys";

		key-mode {
			label = "mode";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
			linux,code = <BTN_0>;
		};

		key-reset {
			label = "reset";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	aliases {
		led-boot = &led_pwr;
		led-failsafe = &led_pwr;
		led-running = &led_pwr;
		led-upgrade = &led_pwr;
	};

	leds {
		compatible = "gpio-leds";

		led_pwr: led-pwr {
			label = "blue:pwr";
			gpios = <&gpio 16 GPIO_ACTIVE_HIGH>;
			default-state = "on";
		};

		led-sfp {
			label = "blue:sfp";
			gpios = <&gpio 9 GPIO_ACTIVE_HIGH>;
		};
	};

	sfp1: sfp1 {
		compatible = "sff,sfp";
		i2c-bus = <&i2c>;
		los-gpios = <&gpio 6 GPIO_ACTIVE_HIGH>;
		mod-def0-gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
		tx-disable-gpios = <&gpio 10 GPIO_ACTIVE_HIGH>;
		maximum-power-milliwatt = <1000>;
	};
};

&mdio {
	ephy7: ethernet-phy@7 {
		reg = <7>;
		sfp = <&sfp1>;
	};
};

&gmac1 {
	phy-mode = "rgmii-rxid";
	phy-handle = <&ephy7>;

	fixed-link {
		status = "disabled";
	};
};

&i2c {
	status = "okay";
};

&switch0 {
	ports {
		port@0 {
			status = "okay";
			label = "wan";
		};

		port@1 {
			status = "okay";
			label = "lan2";
		};

		port@2 {
			status = "okay";
			label = "lan3";
		};

		port@3 {
			status = "okay";
			label = "lan4";
		};

		port@4 {
			status = "okay";
			label = "lan5";
		};

		port@5 {
			status = "disabled";
		};
	};
};

&mmc {
	status = "okay";
};
